<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mars.residentshield">

    <application
        android:allowBackup="true"
        android:icon="@drawable/shield"
        android:label="@string/app_name"
        android:roundIcon="@drawable/shield"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SpalshScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.EnterMobile" />
        <activity android:name=".Activity.ResidentDetails" />
        <activity android:name=".Activity.Home">

        </activity>
    </application>

</manifest>